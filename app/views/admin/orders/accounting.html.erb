<script type="text/javascript">
  function travel_fee_edit(order_id){
    $("#travel_fee_edit"+order_id).dialog({modal: true, height: 100, width: 400})
  }

  function total_revenue_edit(order_id){
    $("#total_revenue_edit"+order_id).dialog({modal: true, height: 100, width: 400})
  }

  function notaryother_fee_edit(order_id){
    $("#notaryother_fee_edit"+order_id).dialog({modal: true, height: 100, width: 400})
  }
</script>
<div class="block" id="block-tables">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><a href="/admin/orders/open_order">Open Orders</a></li>
      <li class="active"><a href="/admin/orders/index">Completed Orders</a></li>
      <!--<li class="first"><a href="/admin/orders/pending_order">Pending Orders/Notary</a></li>-->
      <li class="first"><a href="/admin/orders/order_history_paid">Orders History</a></li>
      <li class="first"><a href="/admin/orders/cancelled_order">Cancelled Orders</a></li>
      <!--<li class="first"><a href="/admin/orders/accounting">Accounting</a></li>-->
    </ul>
    <div class="clear"></div>
  </div>
  <div class="content" id="completed_orders_index">
	<ul class='sub_tab_list'>
    <li class="sub_tab comp-ordr"><%= completed_orders%></li>
    <li class="sub_tab acc"><%= accounting_tab%></li>
</ul>
    <div class="inner">
      <table class="table" id="myTable">
        <colgroup>
          <col id="col1_1"></col>

          <col id="col1_2"></col>
          <col id="col1_3"></col>
          <col id="col1_4"></col>
          <col id="col1_5"></col>
        </colgroup>
        <thead>
          <tr>
            <td>Order #</td>
            <td>Customer Name</td>
            <td>Order Date</td>
            <td>Signing Type</td>
            <td>Signing Date</td>
            <td>Signer Name</td>
            <td>Signing Fee</td>
            <td>Other Fee</td>
            <td>Notary Gross</td>
            <td>Notary Name</td>
            <td>Notary Fee</td>
            <td>Notary Other Fee</td>
            <td>Total Fee</td>
						      <td>Executive Names</td>
						      <td>Executive Fee</td>
            <td>Notary Paid</td>
            <td>NF Net</td>
            <td>Paid Date</td>
          </tr>
        </thead>
        <tbody>
          <% for order in @orders %>

            <tr class="odd">

              <td><a href="javascript:void(0)" onclick="divwin=dhtmlwindow.open('divbox', 'div', 'order_number<%= order.id %>', '', 'width=800px,height=600px,left=200px,top=150px,resize=0,scrolling=10');" title="View Order Details" style="color: #fff;font-size: 12px;"><%= order.id %></a></td>
              <td><a href="javascript:void(0)" onclick="divwin=dhtmlwindow.open('divbox', 'div', 'client_name<%= order.client.client_name %>', '', 'width=800px,height=600px,left=200px,top=150px,resize=0,scrolling=10');" title="View Client Profile" style="color: #fff;font-size: 12px;"><%= order.client.client_name %></a></td>
              <td><%= order.created_at.strftime '%m-%d-%Y'%></td>
              <td><%=signing_type(order.id)%> &nbsp; <%=no_of_doc(order.id)%></td>
              <td> <%if order.signing_date!=nil%><%= order.signing_date.strftime '%m-%d-%Y'%> <%end%></td>
							       <td><%= order.client.client_name%></td>
							       <td><%="$"%><%=number_with_precision(order.client.customer_fee,:precision=>2)%></td>
              <%if !order.travel_fee.blank?%>
                <%travel_fee=order.travel_fee%>
                <td><a href="javascript:void(0)" onClick="travel_fee_edit(<%=order.id%>)" style="color: #fff;font-size: 12px;"><%="$"%><%=number_with_precision(order.travel_fee,:precision=>2)%></a></td>
              <%else%>
                <%travel_fee=0%>
                <td><a href="javascript:void(0)" onClick="travel_fee_edit(<%=order.id%>)" style="color: #fff;font-size: 12px;"><%="$"%><%="0.00"%></a></td>
              <%end%>
														<td>$<%= notary_gross=(order.client.customer_fee.to_i + order.travel_fee.to_i) %></td>
														<% if order.notary.present? %>
                <td><a href="javascript:void(0)" onclick="divwin=dhtmlwindow.open('divbox', 'div', 'notary_name_<%= order.notary.first_name %>_<%= order.notary.last_name %>', '', 'width=800px,height=1100px,left=200px,top=150px,resize=0,scrolling=10');" title="View Notary Profile" style="color: #fff;font-size: 12px;"><%= order.notary.first_name %>&nbsp;<%= order.notary.last_name %></a></td>
																<td><%= order.notary.fee %></td>
																<% other_fee = order.notary.other_fee %>
                <td><a href="javascript:void(0)" onClick="notaryother_fee_edit(<%=order.id%>)" style="color: #fff;font-size: 12px;"><%="$"%><%=number_with_precision(other_fee,:precision=>2)%></a></td>
																<% total_fee = order.notary.fee.to_i + other_fee.to_i %>
						        <% else %>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>$0.0</td>
																<% total_fee = 0 %>
														<% end %>
														<td><%="$"%><%=number_with_precision(total_fee, :precision=>2)%></td>
														<% executive_fee = 0%>
              <% if order.client.executives.present? %>
																<% executive = order.client.client_executives.first %>
																<% if executive.share_percentage.present? %>
																			<% executive_fee = ( order.client.customer_fee.to_i / 100.0 ) * executive.share_percentage %>
																<% else %>
																			<% executive_fee = executive.share_value %>
																<% end %>
                <td>
																								<% order.client.executives.each do |e| %>
																																<small><%= e.name %></small><br/>
																								<% end %>
                </td>
																<td>$<%= executive_fee %></td>
              <% else %>
                <td/>
                <td/>
              <% end %>
														<!--<td><%#="$"%><%#=number_with_precision(order.client.customer_fee,:precision=>2)%></td>-->
              <%total_revenue=order.client.customer_fee%>
              <%if order.notary_id!=nil%>
                <td><%="$ "%><%=number_with_precision(order.notary_fee,:precision=>2)%></td>
                <%doc_notary_fee=order.notary_fee%>
              <%else%>
                <%doc_notary_fee=0%>
							         <td></td>
              <%end%>
              <!--<td><%#="$ "%><%#=number_with_precision(total_revenue.to_f-(travel_fee.to_f+ doc_notary_fee.to_f),:precision=>2)%></td>-->
              <td><%="$ "%><%=number_with_precision(notary_gross.to_f-(total_fee.to_f+executive_fee),:precision=>2)%></td>
              <%if order.order_notary_fee_date!=nil%>
                <td><%=  order.order_notary_fee_date.strftime '%m-%d-%Y' %></td>
              <%else%>
                <td>Not paid</td>
              <%end%>
             </tr>

										<% if order.notary.present? %>
          <div id="notaryother_fee_edit<%=order.id%>" style="display:none;" >
            <%form_tag ("/admin/orders/edit_notaryother_fee")do %>
              Notary Other fee: <%= text_field_tag 'notaryother_fee', other_fee %>
              <%= hidden_field_tag 'notary_id', order.notary.id %>&nbsp;<%=submit_tag 'submit'%>
            <%end%>
          </div>
        		<% end %>

          <div id="travel_fee_edit<%=order.id%>" style="display:none;" >
            <%form_tag ("/admin/orders/edit_travel_fee")do %>
              Travel fee: <%= text_field_tag 'travel_fee','' %>
              <%= hidden_field_tag 'order_id',order.id %>&nbsp;<%=submit_tag 'submit'%>
            <%end%>
          </div>

          <div id="total_revenue_edit<%=order.id%>" style="display:none;" >
            <%form_tag ("/admin/orders/edit_total_revenue")do %>
              Total Revenue: <%= text_field_tag 'total_revenue','' %>
              <%= hidden_field_tag 'order_id',order.id %>&nbsp;<%=submit_tag 'submit'%>
            <%end%>
          </div>


          <div id="client_name<%= order.client.client_name %>" style="display:none">
            <div id="order_details" class="border span-18 last" style="border:none">
              <div class="xsnazzy" style="width: 740px">
                <b class="xtop"><b class="xb1"></b><b class="xb2 color_a">
                  </b><b class="xb3 color_a"></b><b class="xb4 color_a"></b></b>
                <div class="xboxcontent" style="padding:10px; margin: 0px">
                  <ul class="notary_info">
                    <p>
                    <li><label>Client ID:</label> <%= order.client.id %></li>
                    <li><label>Company Name:</label> <%= order.client.company_name %></li>
                    <li><label>Address:</label> </li>
                    <li><%= order.client.address %><br/> <%= order.client.city %>, <%= order.client.state %> <%= order.client.zip_code%></li>
                    <li><label>Name:</label> <%= order.client.client_name %></li>
                    <li><label>Title:</label><% if order.client.title.length!=0%> <%= order.client.title %><%else%> Not Provided<%end%></li>
                    </p>
                    <p>
                    <li><label>Telephone:</label><% if order.client.home_phone.length!=0%> <%= order.client.home_phone %><%else%> Not Provided<%end%></li>
                    <li><label>Mobile:</label><% if order.client.mobile_phone.length!=0%> <%= order.client.mobile_phone %><%else%> Not Provided<%end%></li>
                    <li><label>Fax:</label><% if order.client.fax_number.length!=0%> <%= order.client.fax_number %><%else%> Not Provided<%end%></li>

                    </p>
                    <p>
                    <li><label>Return Docs To:</label></li>
                    <li><%= order.return_company_name %><br/><label> Attn:</label> <%= order.return_attention %><br/> <%= order.return_address %><br/> <%= order.return_city%>,<%= order.return_state%> <%= order.return_zip_code%> <br/> <%= order.return_shipping_courier%><label> Acct#:</label> <%= order.return_account_number%></li>
                    </p>
                  </ul>
                </div>
                <b class="xbottom"><b class="xb4"></b><b class="xb3"></b>
                  <b class="xb2"></b><b class="xb1"></b></b>
              </div>
            </div>
          </div>


          <% if order.notary!=nil %>
