                          <li style="margin-left:5px;"><label>Power of Attorney:</label> <%= order.notary.power_of_attorney %></li>
                          <li style="margin-left:5px;"><label>Reverse Mortgage?:</label> <%= order.notary.lines_of_credit %></li>
