                        <%= order.order_notary_payment_address %><br /><%= order.order_notary_payment_city  %>, <%= order.order_notary_payment_state %> <%= order.order_notary_payment_zip_code %><li>
                        </li>
                      </ul>
                    </div>
                    <div class="span-8 last">
    <%# if order.notary.photo_file_name %><p><%#= image_tag order.notary.photo.url, :height => 75, :style => "float:right; border:2px solid #000;" %></p><%# end %>
                    </div>
                  </div>
                  <b class="xbottom"><b class="xb4"></b><b class="xb3"></b>
                    <b class="xb2"></b><b class="xb1"></b></b>
                </div>
              </div>
            </div>
          <%else%>

          <% end %>


