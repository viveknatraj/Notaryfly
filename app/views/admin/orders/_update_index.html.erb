<script class="jsbin" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
<meta charset=utf-8/>
<script>
  function get_checkbox(order_id, div_window) {
    var checkboxes = document.getElementsByName("order_" + order_id + "[executives][]");
    var exec_list = new Array();
    var j = 0;
    for (var i = 0, n = checkboxes.length; i < n; i++) {
      if (checkboxes[i].checked)
        exec_list[j++] = checkboxes[i].value;
    }
    //var params={'order' : {'id': order_id, 'executives' : "["+[exec_list]+"]"}};
    var url = "/admin/orders/update_executive";
    $.post(url, {order_id: order_id, executives: [exec_list]}).success(function (text) {
      if (text.match(/Error.*/)) alert(text); else {
        alert(text);
        div_window.close();
        window.location.reload();
      }
    });
  }
  function toggle_checkboxes(check) {
    var checkboxes = document.getElementsByName("order_ids[]");
    if (check.checked) {
      for (var i = 0, n = checkboxes.length; i < n; i++)
        checkboxes[i].checked = true;
    }
    else {
      for (var i = 0, n = checkboxes.length; i < n; i++)
        checkboxes[i].checked = false;
    }
  }
  function check_payment(event) {
    var checkboxes = document.getElementsByName("order_ids[]");
    for (var i = 0, n = checkboxes.length; i < n; i++) {
      if (checkboxes[i].checked)
        return true;
    }
    alert('Please select any of the order to make payment');
    return false;
  }

  $(document).ready(function() {
    $("#switch-view").change(function() {
      $("#orders-index").submit();
    });
  });
</script>


